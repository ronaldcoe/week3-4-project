{
  "swagger": "2.0",
  "info": {
    "title": "Clinic API",
    "description": "The Clinic API provides the endpoints for the clinic API",
    "version": "1.0.0"
  },
  "host": "localhost:3000",
  "basePath": "/",
  "schemes": [
    "http"
  ],
  "paths": {
    "/patients": {
      "get": {
        "tags": [
          "Patients"
        ],
        "description": "Endpoint to get all patients",
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/patients/{id}": {
      "get": {
        "tags": [
          "Patients"
        ],
        "description": "Endpoint to get a patient by ID",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/patients/create_patient": {
      "post": {
        "tags": [
          "Patients"
        ],
        "description": "Endpoint to create a patient",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "description": "Object containing the patient information",
            "schema": {
              "type": "object",
              "properties": {
                "firstName": {
                  "type": "string",
                  "example": "John"
                },
                "lastName": {
                  "type": "string",
                  "example": "Doe"
                },
                "dateOfBirth": {
                  "type": "string",
                  "example": "1990-01-01"
                },
                "gender": {
                  "type": "string",
                  "example": "Male"
                },
                "contactNumber": {
                  "type": "string",
                  "example": "801-555-5555"
                },
                "emailAddress": {
                  "type": "string",
                  "example": "jhon@mail.com"
                },
                "address": {
                  "type": "string",
                  "example": "123 Main St."
                },
                "allergies": {
                  "type": "array",
                  "example": [
                    "Peanuts"
                  ],
                  "items": {
                    "type": "string"
                  }
                },
                "medicalHistory": {
                  "type": "string",
                  "example": "None"
                }
              },
              "required": [
                "firstName",
                "lastName",
                "dateOfBirth",
                "gender",
                "contactNumber",
                "emailAddress",
                "address",
                "allergies",
                "medicalHistory"
              ]
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Created"
          },
          "400": {
            "description": "Bad Request"
          },
          "401": {
            "description": "Unauthorized"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    },
    "/patients/delete_patient/{id}": {
      "delete": {
        "tags": [
          "Patients"
        ],
        "description": "Endpoint to delete a patient",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "404": {
            "description": "Not Found"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    },
    "/patients/update_patient/{id}": {
      "put": {
        "tags": [
