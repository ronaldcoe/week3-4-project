                  "example": "123 Main St."
                },
                "allergies": {
                  "type": "array",
                  "example": [
                    "Peanuts"
                  ],
                  "items": {
                    "type": "string"
                  }
                },
                "medicalHistory": {
                  "type": "string",
                  "example": "None"
                }
              },
              "required": [
                "firstName",
                "lastName",
                "dateOfBirth",
                "gender",
                "contactNumber",
                "emailAddress",
                "address",
                "allergies",
                "medicalHistory"
              ]
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "400": {
            "description": "Bad Request"
          },
          "404": {
            "description": "Not Found"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    },
    "/appointments": {
      "get": {
        "tags": [
          "Appointments"
        ],
        "description": "Endpoint to get all appointments",
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/appointments/{id}": {
