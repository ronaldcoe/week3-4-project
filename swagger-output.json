        ],
        "description": "Endpoint to create an appointment",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "description": "Object containing the appointment information",
            "schema": {
              "type": "object",
              "properties": {
                "doctorId": {
                  "type": "string",
                  "example": "D3002"
                },
                "appointmentDate": {
                  "type": "string",
                  "example": "2021-03-01T08:00:00.000Z"
                },
                "purpose": {
                  "type": "string",
                  "example": "Checkup"
                },
                "status": {
                  "type": "string",
                  "example": "Scheduled"
                },
                "notes": {
                  "type": "string",
                  "example": "Regular checkup"
                },
                "patientId": {
                  "type": "string",
                  "example": "6558240a4dd4112f37b23141"
                }
              },
              "required": [
                "doctorId",
                "appointmentDate",
                "purpose",
                "status",
                "notes",
                "patientId"
              ]
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Created"
          },
          "400": {
